<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Suppress false positives for Spring Boot dependencies -->
    <suppress>
        <notes><![CDATA[
        This suppresses false positives for Spring Boot starter dependencies
        that are known to be safe in this context.
        ]]></notes>
        <gav regex="true">^org\.springframework\.boot:spring-boot-starter.*:.*$</gav>
        <cve>CVE-2016-1000027</cve>
    </suppress>
    
    <!-- Suppress MySQL connector false positives -->
    <suppress>
        <notes><![CDATA[
        MySQL connector may have false positives for older vulnerabilities
        that don't apply to our usage pattern.
        ]]></notes>
        <gav regex="true">^mysql:mysql-connector-java:.*$</gav>
        <cve>CVE-2019-2692</cve>
    </suppress>
</suppressions>